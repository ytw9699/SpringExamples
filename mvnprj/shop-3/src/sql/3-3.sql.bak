CREATE TABLE USER_ACCOUNT(
	USER_ID VARCHAR2(20),
	USER_NAME VARCHAR2(20),
	PASSWORD VARCHAR2(20),
	POSTCODE VARCHAR2(8),
	ADDRESS VARCHAR2(50),
	EMAIL VARCHAR2(50),
	JOB VARCHAR2(30),
    BIRTHDAY DATE,
	CONSTRAINT USER_ID_PK PRIMARY KEY (USER_ID)
);

